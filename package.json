{
  "name": "@hyperone/cli",
  "version": "1.4.0",
  "description": "HyperOne Command Line Interface",
  "license": "MIT",
  "author": "",
  "main": "bin/start",
  "bin": {
    "h1": "./bin/h1",
    "rbx": "./bin/rbx"
  },
  "scripts": {
    "start": "./bin/h1.js",
    "build": "pkg -c package.json -o ./dist/h1 ./bin/h1 && pkg -c package.json -o ./dist/rbx ./bin/rbx",
    "lint": "eslint .",
    "docs": "node scripts/print_docs/index.js docs/",
    "docs_coverage": "node scripts/print_docs_coverage.js",
    "test": "npx ava --verbose"
  },
  "dependencies": {
    "@hyperone/vhdx": "^1.0.1",
    "chalk": "^2.4.2",
    "cookie": "^0.3.1",
    "dns-zonefile": "^0.2.2",
    "inquirer": "^6.2.1",
    "jmespath": "^0.15.0",
    "js-yaml": "^3.12.1",
    "lodash": "^4.17.11",
    "marked": "^0.4.0",
    "marked-terminal": "^3.1.1",
    "node-rsa": "^0.4.2",
    "opn": "^5.4.0",
    "progress": "^2.0.3",
    "qrcode": "^1.3.2",
    "request": "^2.88.0",
    "sha512crypt-node": "^0.1.0",
    "shell-quote": "^1.6.1",
    "speakeasy": "^2.0.0",
    "ssh2": "^0.6.2",
    "structured-cli": "^1.0.5",
    "superagent": "^4.1.0",
    "tabula": "^1.10.0",
    "websocket-stream": "^5.1.2",
    "ws": "^6.1.2",
    "ws-streamify": "^0.1.4"
  },
  "pkg": {
    "scripts": [
      "lib/**/*.js",
      "bin/**/*.js",
      "!bin/**/tests.js",
      "!lib/**/tests.js"
    ],
    "assets": [
      "bin/**/*.md",
      "bin/**/*.ps",
      "bin/replica/create/HyperOneCa.cer"
    ],
    "targets": [
      "node10-alpine",
      "node10-linux",
      "node10-macos",
      "node10-win"
    ]
  },
  "keywords": [],
  "devDependencies": {
    "@hyperone/eslint-config": "^1.0.3",
    "ajv": "^6.7.0",
    "ava": "^1.0.1",
    "eslint": "^4.19.1",
    "fast-glob": "^2.2.6",
    "imap": "^0.8.19",
    "node-forge": "^0.7.6",
    "nodemailer": "^4.7.0",
    "pkg": "^4.3.5"
  },
  "ava": {
    "files": [
      "bin/**/tests.js",
      "lib/**/tests.js",
      "!lib/tests.js"
    ]
  }
}
