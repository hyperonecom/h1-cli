{
  "name": "h1-cli",
  "version": "0.4.0",
  "description": "HyperOne Command Line Interface",
  "license": "MIT",
  "author": "",
  "main": "bin/h1",
  "bin": {
    "h1": "./bin/h1"
  },
  "scripts": {
    "start": "./bin/h1",
    "build": "pkg --out-path ./dist ."
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "cookie": "^0.3.1",
    "inquirer": "^1.2.3",
    "jmespath": "^0.15.0",
    "lodash": "^4.17.5",
    "node-rsa": "^0.4.2",
    "opn": "^5.3.0",
    "progress": "^2.0.0",
    "rbx-vhdx": "git+ssh://git@bitbucket.org/rbxcom/rbx-vhdx.git",
    "request": "^2.85.0",
    "ssh2": "^0.6.0",
    "structured-cli": "^1.0.5",
    "superagent": "^2.3.0",
    "tabula": "^1.10.0",
    "websocket-stream": "^5.1.2",
    "ws": "^5.1.1",
    "ws-streamify": "^0.1.3"
  },
  "pkg": {
    "scripts": [
      "lib/**/*.js",
      "bin/**/*.js"
    ],
    "targets": [
      "node8-alpine",
      "node8-linux",
      "node8-macos",
      "node8-win"
    ]
  },
  "keywords": [],
  "devDependencies": {
    "eslint": "^4.19.1",
    "pkg": "^4.3.1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 8
    },
    "env": {
      "browser": false,
      "node": true
    },
    "rules": {
      "quotes": ["error", "single"],
      "comma-style": ["error", "first"],
      "comma-spacing": ["error", { "before": false, "after": true }]
    }
  }
}
