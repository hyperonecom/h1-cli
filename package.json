    "docs_coverage": "node scripts/print_docs_coverage.js",
    "test": "npx ava --verbose"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "cookie": "^0.3.1",
    "inquirer": "^1.2.3",
    "jmespath": "^0.15.0",
    "lodash": "^4.17.5",
    "marked": "^0.4.0",
    "marked-terminal": "^3.0.0",
    "node-rsa": "^0.4.2",
    "opn": "^5.3.0",
    "progress": "^2.0.0",
    "qrcode": "^1.2.0",
    "rbx-vhdx": "https://bitbucket.org/rbxcom/rbx-vhdx/get/master.tar.gz",
    "request": "^2.85.0",
    "shell-quote": "^1.6.1",
    "speakeasy": "^2.0.0",
    "ssh2": "^0.6.0",
    "structured-cli": "^1.0.5",
    "superagent": "^2.3.0",
    "tabula": "^1.10.0",
    "websocket-stream": "^5.1.2",
    "ws": "^5.1.1",
    "ws-streamify": "^0.1.3"
  },
  "pkg": {
    "scripts": [
      "lib/**/*.js",
      "bin/**/*.js"
    ],
    "targets": [
      "node8-alpine",
      "node8-linux",
      "node8-macos",
      "node8-win"
    ]
  },
  "keywords": [],
  "devDependencies": {
    "ava": "1.0.0-beta.5.1",
    "eslint": "^4.19.1",
    "nodemailer": "^4.6.7",
    "pkg": "^4.3.1"
  },
  "ava": {
    "files": [
      "bin/**/tests.js"
    ]
  }
